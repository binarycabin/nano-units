{
    "name": "binarycabin/nano-units",
    "description": "A simple helper for converting units of Nano cryptocurrency",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Kilroy",
            "email": "jeff@binarycabin.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BinaryCabin\\NanoUnits\\": "src/"
        }
